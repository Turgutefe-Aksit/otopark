@using MVCTrainProject.Models
@model List<park_place>
@{ ViewBag.Title = "CustomerIndex";
                Layout = "~/Views/Shared/_CustomerLayout.cshtml"; }

<!DOCTYPE html>

<html>
<head>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.6.0.min.js"></script>
    <title>Index</title>

</head>
<body>

    <div class="dis_bolme">
        <div class="ic_bolme" style="float:left; padding:30px;">

            <table>
                @foreach (var park_place in Model)
                {
                    if (park_place.loc_number <= 5)
                    {
    <tr>
        <td><div style="width: 150px; height: 50px; background-color: white; border-radius: 10px">Park @park_place.loc_number <b>@park_place.occupancy_info </b></div></td>

    </tr>
                        <tr>
                            <td> <br /> </td>
                        </tr>}
                                    }
            </table>
        </div>

        <div class="ic_bolme" style="padding:30px">

            <table>
                @foreach (var park_place in Model)
                {
                    if (park_place.loc_number > 5)
                    {
    <tr>
        <td><div style="width:150px; height:50px; background-color:white; border-radius:10px">Park @park_place.loc_number <b>@park_place.occupancy_info </b></div></td>
    </tr>
                        <tr>
                            <td> <br /> </td>
                        </tr>}
                                    }

            </table>

        </div>
    </div>
</body>
</html>

