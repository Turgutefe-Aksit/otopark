@model MVCTrainProject.Models.Classes.IEnumClass
@{ ViewBag.Title = "AddCustomer"; }

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.6.0.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <script>

        function Alert() {

            alert("Kayıt Başarıyla Eklendi");

        }
    </script>
</head>
<body>
    <h2 style="color:white">ADD PARK REGISTRATION / OTOPARK KAYDI OLUŞTURMA</h2>
    <form method="post">

        <label style="color:white"><b>Araç Plakası</b></label>
        @Html.DropDownListFor(c => c.carClass.car_plate, (List<SelectListItem>)ViewBag.vl, new { @class = "form-control" });
        <label style="color:white"><b>Park Yeri</b></label>
        @Html.DropDownListFor(c => c.parkClass.loc_number, (List<SelectListItem>)ViewBag.vl2, new { @class = "form-control" });

        <br />
        <button class="btn btn-info" onclick="Alert()">Kayıt Ekle / Add Registration</button>


    </form>
</body>
